&qupv3_hub_i2c5 {
	s2miw04-charger@3b {
		status = "disabled";
	};

	s2miw04-charger@5b {
		compatible = "lsi,s2miw04-charger";
		reg = <0x5b>;
		status = "okay";

		pinctrl-names = "default";
		pinctrl-0 = <&lsi_irq_default &lsi_det_default &lsi_en_default  &lsi_pdrc_default>;

		battery,wpc_int = <SEC_GPIO_REF(AP,tlmm,98) 1>; /* MFC_AP_INT */
		battery,wpc_det = <SEC_GPIO_REF(PM,pm8550_gpios,9) 0>; /* WPC_DET */
#if 0
		battery,mst_pwr_en = <SEC_GPIO_REF(${lsi_pwr_en_gpio}) 0>; /* MST_PWR_EN */
#endif
		battery,wpc_en = <SEC_GPIO_REF(AP,tlmm,167) 0>; /* WPC_EN */
		battery,wpc_pdrc = <SEC_GPIO_REF(PM,pm8550ve_f_gpios,7) 1>; /* VRECT_INT */

		battery,charger_name = "max77705-charger";
		battery,fuelgauge_name = "max77705-fuelgauge";
		battery,wireless_charger_name = "s2miw04-charger";
		battery,wc_cover_rpp = <0x44>;
		battery,phone_fod_threshold = <0x3b>;
		battery,wireless20_vout_list = <WIRELESS_VOUT_9V
					WIRELESS_VOUT_11V
					WIRELESS_VOUT_11V
					WIRELESS_VOUT_11V
					WIRELESS_VOUT_11V>;
		battery,wireless20_vrect_list = <MFC_AFC_CONF_12V_TX
						MFC_AFC_CONF_12_5V_TX
						MFC_AFC_CONF_12_5V_TX
						MFC_AFC_CONF_12_5V_TX
						MFC_AFC_CONF_12_5V_TX>;
		battery,wireless20_max_power_list = <SEC_WIRELESS_RX_POWER_12W
						SEC_WIRELESS_RX_POWER_15W
						SEC_WIRELESS_RX_POWER_15W
						SEC_WIRELESS_RX_POWER_15W
						SEC_WIRELESS_RX_POWER_15W>;
		battery,oc_fod1 = <1100>;
		battery,phone_fod_thresh1 = <0x0ED8>; /* 3800 */
		battery,buds_fod_thresh1 = <0x0ED8>; /* 3800 */
		battery,ping_freq = <1470>;
		battery,wpc_vout_ctrl_full = <WIRELESS_VOUT_5V_STEP>;
		battery,tx_gear_min_op_freq_delay = <4000>;
		battery,mis_align_guide;
		battery,mis_align_target_vout = <5000>;
		battery,unknown_cmb_ctrl;
		battery,default_clamp_volt;

		fod_list {
			hv_fod_cc = <0xFF 0xFF 0xFF 0xE8 0x81 0x6D 0x50 0x50>;
			hv_fod_cv = <0xFF 0xFF 0xFF 0xE8 0x81 0x6D 0x50 0x50>;
			hv_fod_full = <0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF 0xFF>;

			count = <1>;
			pad_0x00 { /* DEFAULT */
				flag = <(SET_FOD_CC(ADD) | SET_FOD_CV(ADD) | SET_FOD_FULL(ADD))>;
				cc = <0x09 0xD0 0x09 0xA8 0x09 0xA8 0x0C 0x8C 0x0C 0x8C
					0x0C 0x8C 0x1A 0x5A 0x1A 0x5A 0x19 0x5E 0x19 0x5E>;
				cv = <0x09 0xD0 0x09 0xBC 0x09 0xBC 0x0C 0xA0 0x0C 0xA0
					0x0C 0xA0 0x1A 0x6E 0x1A 0x6E 0x19 0x72 0x19 0x72>;
				full = <0x33 0xFF 0x33 0xFF 0x33 0xFF 0x33 0xFF 0x33 0xFF
					0x33 0xFF 0x33 0xFF 0x33 0xFF 0x33 0xFF 0x33 0xFF>;
			};
		};

		iec_data {
			reg_56 = <0x32>;
			reg_57 = <0x1E>;
			reg_5B = <0x1E>;
			reg_84 = <0x5A>;
			reg_85 = <0x1B>;
			reg_86 = <0x09>;
			reg_87 = <0x0C>;
			reg_88 = <0xA6>;
			reg_89 = <0x3C>;
			reg_8A = <0x78>;
			reg_8B = <0x5F>;
			reg_800 = <0x28>;
			reg_801 = <0x46>;
			reg_802 = <0x8C>;
		};
	};
};
